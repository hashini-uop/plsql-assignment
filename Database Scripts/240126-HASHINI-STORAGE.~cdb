CREATE TABLE HASHINI_STORAGE_TAB (
    store_id VARCHAR2(100),
    container_id VARCHAR2(30),
    item_id VARCHAR2(100),
    status VARCHAR2(50),
    PRIMARY KEY(store_id),
    CONSTRAINT fk_storage_container FOREIGN KEY (container_id, item_id) REFERENCES HASHINI_CONTAINER_TAB(container_id, item_id),
    CONSTRAINT fk_storage_item FOREIGN KEY (item_id) REFERENCES HASHINI_FOOD_ITEM_TAB(item_id)
);
