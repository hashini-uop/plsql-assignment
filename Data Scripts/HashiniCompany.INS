DECLARE
  company_name VARCHAR2(200);
  company_address VARCHAR2(4000);
BEGIN
    HASHINI_COMPANY_API.insert_company('Oppo', '1 Wve Street, Kadawatha');

    company_name := HASHINI_COMPANY_API.get_company_name(2);
    DBMS_OUTPUT.PUT_LINE('Company Name: ' || company_name);
    
    company_address := HASHINI_COMPANY_API.get_company_address(3); 
    DBMS_OUTPUT.PUT_LINE('Company Address: ' || company_address);


END;
